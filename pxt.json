{
    "name": "neopixel-wearables",
    "version": "0.0.5",
    "description": "Addition blocks for using the AdaFruit NeoPixel driver with complex patterns of lights and animations",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "ws2812b": "github:Microsoft/pxt-ws2812b#v0.0.4"
    },
    "files": [
        "README.md",
        "neopixel.ts",
        "layoutdesigner.ts",
        "_locales/ja/neopixel-strings.json",
        "_locales/de/neopixel-strings.json",
        "_locales/de/neopixel-jsdoc-strings.json"
    ],
    "testFiles": [
        "neotest.ts"
    ],
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 0
                }
            }
        }
    },
    "public": true,
    "extension": {
        "namespace": "layoutdesigner",
        "label": "Neopixel Layout Designer",
        "localUrl": "http://127.0.0.1:8080/index.html?d"
    }
}